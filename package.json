{
  "name": "boilerplate_express_framework_es6",
  "version": "1.0.0",
  "description": "ES6 Express Framework for Enterprise Applications ()\u001b[DBoilerplate)",
  "keywords": [
    "ES6",
    "Framework",
    "Arslan",
    "Ismail",
    "BoilerPlate",
    "Enterprise",
    "Application"
  ],
  "author": "Arslan Ismail",
  "main": "bin/start.js",
  "scripts": {
    "start": "npm run start:prod",
    "build": "babel ./ --out-dir dist --ignore 'node_modules/**/*.js'",
    "server": "babel-node ./bin/start.js",
    "server:prod": "node ./dist/bin/start.js",
    "start:dev": "cross-env NODE_ENV=development nodemon --ignore './public/' --exec npm run server",
    "start:prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "lint": "eslint \"**/*.js\"",
    "clean": "rimraf dist"
  },
